{
    "manifest_version": 3,
    "name": "Scrape Text Extension",
    "version": "1.0",
    "description": "Scrapes text from h1-h6 and div tags with font size greater than 15px",
    "permissions": [
      "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["<alls _urls>"],
        "js": ["content.js"]
      }
    ]
  } 
  